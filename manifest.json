{
  "default_locale": "en",
  "name": "__MSG_ext_name__",
  "short_name": "__MSG_ext_shortname__",
  "description": "__MSG_ext_description__",
  "version": "1.1.2",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*", "https://*.youtube.com/*"],
      "all_frames": true,
      "js": ["js/build/langs.js", "js/build/bg.js", "js/build/config.js"]
    }
  ],
  "web_accessible_resources": ["css/style.css"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"]
}
