{
  "name": "Youtube Comment Language Filter",
  "description": "Filter comments by your language only",
  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "http://*.youtube.com/*",
        "https://*.youtube.com/*"
      ],
      "all_frames": true,
      "js": [
        "js/bg.js"
      ]
    }
  ],
  "permissions": ["tabs"]
}
